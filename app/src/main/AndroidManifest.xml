<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.money">

    <application
        android:allowBackup="true"
        android:icon="@drawable/money"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitisopen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitisopen.SesionActivity"
            android:parentActivityName=".activitisopen.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activitisopen.MainActivity" />
        </activity>
        <activity
            android:name=".activitisopen.RegisterActivity"
            android:parentActivityName=".activitisopen.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activitisopen.MainActivity" />
        </activity>
        <activity android:name=".activities.NavegacionActivity" />
        <activity android:name=".activities.CategoriaActivity" />
        <activity android:name=".activities.GraficoActivity" />
        <activity android:name=".listas.ListaIngresos"
            android:parentActivityName=".activitisopen.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activitisopen.MainActivity" />
        </activity>
        <activity android:name=".listas.ListaEgresos"
            android:parentActivityName=".activitisopen.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activitisopen.MainActivity" />
        </activity>
        <activity android:name=".listas.ListaCategoria"
            android:parentActivityName=".activitisopen.MainActivity">
            <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".activitisopen.MainActivity" />
        </activity>
        <activity android:name=".listas.ListaUsuario" />
    </application>

</manifest>